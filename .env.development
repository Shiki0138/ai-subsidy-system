# 開発環境設定
NODE_ENV=development

# 認証無効化（開発用）
DISABLE_AUTH=true

# データベース設定（開発用）
DATABASE_URL="postgresql://devuser:devpass@localhost:5432/ai_subsidy_dev?schema=public"

# Redis設定（開発用）
REDIS_URL="redis://localhost:6379"

# JWT設定（開発時は固定値）
JWT_SECRET="dev-secret-key-not-for-production"
JWT_REFRESH_SECRET="dev-refresh-secret-not-for-production"

# AI API設定（テスト用キー）
OPENAI_API_KEY="sk-test-key-replace-with-real-key"
ANTHROPIC_API_KEY="sk-ant-test-key-replace-with-real-key"
AI_PROVIDER_PREFERENCE="openai"

# 開発サーバー設定
PORT=7001
FRONTEND_URL=http://localhost:7002

# ログ設定
LOG_LEVEL=debug
LOG_TO_FILE=false

# セキュリティ設定（開発用）
SESSION_SECRET="dev-session-secret"
ENCRYPTION_KEY="dev-encryption-key-32-characters"

# ファイルアップロード設定
UPLOAD_DIR="./uploads"
MAX_FILE_SIZE=10485760

# レート制限（開発時は緩い設定）
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# 開発モード特有の設定
ENABLE_SWAGGER=true
ENABLE_DEBUG_ROUTES=true
MOCK_AI_RESPONSES=false
DISABLE_HTTPS_REDIRECT=true

# テストユーザー情報
TEST_USER_EMAIL="dev@ai-subsidy.test"
TEST_USER_PASSWORD="devpass123"