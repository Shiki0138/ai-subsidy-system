{
  "name": "ai-subsidy-backend",
  "version": "1.0.0",
  "description": "AI補助金申請書自動作成システム - バックエンドAPI",
  "private": true,
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:server": "NODE_ENV=test node simple-api.js",
    "test:integration": "NODE_ENV=test node ../ai-pdf-test.js",
    "test:env": "cp .env.test .env && echo 'テスト環境変数設定完了'",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:migrate:test": "DATABASE_URL='postgresql://ai_user:ai_password_123@localhost:5433/ai_subsidy_test?schema=public' prisma db push --force-reset",
    "db:seed": "ts-node prisma/seed.ts",
    "db:seed:test": "NODE_ENV=test ts-node prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:studio:test": "DATABASE_URL='postgresql://ai_user:ai_password_123@localhost:5433/ai_subsidy_test?schema=public' prisma studio",
    "db:reset": "prisma migrate reset",
    "docker:build": "docker build -t ai-subsidy-backend .",
    "docker:test:up": "docker-compose -f ../docker-compose.test.yml up -d",
    "docker:test:down": "docker-compose -f ../docker-compose.test.yml down --volumes",
    "docker:test:logs": "docker-compose -f ../docker-compose.test.yml logs",
    "security:audit": "npm audit && audit-ci --config audit-ci.json",
    "generate:pdf-templates": "ts-node src/scripts/generateMockPdfTemplates.ts",
    "test:pdf-filling": "ts-node src/scripts/testPdfFilling.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.17.1",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@langchain/anthropic": "^0.3.22",
    "@langchain/core": "^0.3.58",
    "@langchain/openai": "^0.5.13",
    "@prisma/client": "^5.8.1",
    "@types/handlebars": "^4.0.40",
    "@types/nodemailer": "^6.4.17",
    "@types/socket.io": "^3.0.1",
    "@types/stripe": "^8.0.416",
    "axios": "^1.6.5",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "bullmq": "^5.1.3",
    "cheerio": "^1.1.0",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "csurf": "^1.11.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.3",
    "openai": "^4.104.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdf-lib": "^1.17.1",
    "puppeteer": "^21.11.0",
    "puppeteer-core": "^24.10.1",
    "rate-limit-redis": "^4.2.1",
    "redis": "^4.6.12",
    "sharp": "^0.33.2",
    "socket.io": "^4.8.1",
    "stripe": "^18.2.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-elasticsearch": "^0.17.4",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/hpp": "^0.2.5",
    "@types/jest": "^29.5.11",
